{
  "name": "tanstack-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite dev --port 3000",
    "build": "bun --bun vite build",
    "preview": "bun --bun vite preview",
    "test": "bun --bun vitest run",
    "format": "bun --bun biome format",
    "lint": "bun --bun biome lint",
    "check": "bun --bun biome check",
    "post-cta-init": "npx create-db@latest --user-agent tanstack/tsrouter",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- bun --bun prisma db push",
    "db:migrate": "dotenv -e .env.local -- bun --bun prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- bun --bun prisma studio",
    "db:seed": "dotenv -e .env.local -- bun --bun prisma db seed"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/eden": "^1.4.6",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@mantine/core": "^8.3.13",
    "@mantine/dates": "^8.3.13",
    "@mantine/form": "^8.3.13",
    "@mantine/hooks": "^8.3.13",
    "@mantine/notifications": "^8.3.13",
    "@modelcontextprotocol/sdk": "1.24.3",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@t3-oss/env-core": "^0.13.10",
    "@tabler/icons-react": "^3.36.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.157.16",
    "@tanstack/react-router-devtools": "^1.157.16",
    "@tanstack/react-router-ssr-query": "^1.157.16",
    "@tanstack/react-start": "^1.157.16",
    "@tanstack/router-plugin": "^1.157.16",
    "better-auth": "^1.4.18",
    "dayjs": "^1.11.19",
    "elysia": "^1.4.22",
    "lucide-react": "^0.561.0",
    "nitro": "npm:nitro-nightly@latest",
    "pg": "^8.17.2",
    "prisma": "^7.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "valtio": "^2.3.0",
    "vite-tsconfig-paths": "^6.0.5",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tanstack/devtools-vite": "^0.3.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.19.7",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv-cli": "^10.0.0",
    "jsdom": "^27.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0",
    "@types/pg": "^8.16.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1"
  }
}